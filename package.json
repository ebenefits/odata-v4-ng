{
  "name": "odata-v4-ng",
  "version": "1.1.0",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular 7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardomariani/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular",
    "angular 7"
  ],
  "author": "Riccardo Mariani",
  "bugs": {
    "url": "https://github.com/riccardomariani/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/riccardomariani/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "versioningLib": "cd projects/odata-v4-ng && npm version 1.1.0 --allow-same-version && cd ../..",
    "buildLib": "ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --prod",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --prod --deploy-url=odata-v4-ng/ && rm -R -f docs && mv dist/odata-v4-ng-app docs",
    "versioningApp": "git add -A && git commit -m 'docs: update' && standard-version --release-as 1.1.0",
    "buildAll": "npm run versioningLib && npm run buildLib && npm run buildApp && npm run versioningApp",
    "publishApp": "git push --follow-tags origin master",
    "publishLib": "npm publish dist/odata-v4-ng",
    "publishAll": "npm run publishApp && npm run publishLib",
    "release": "cp README.md projects/odata-v4-ng/README.md && npm run buildAll && npm run publishAll"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular2-uuid": "^1.1.0",
    "core-js": "^2.5.4",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "rxjs-tslint": "^0.1.5",
    "standard-version": "^4.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
