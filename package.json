{
  "name": "odata-v4-ng",
  "version": "1.0.15",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular 6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardomariani/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular",
    "angular 6"
  ],
  "author": "Riccardo Mariani",
  "bugs": {
    "url": "https://github.com/riccardomariani/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/riccardomariani/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "versioningLib": "cd projects/odata-v4-ng && npm version 1.0.15 --allow-same-version && cd ../..",
    "buildLib": "ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --prod",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --prod --deploy-url=odata-v4-ng/ && rm -R -f docs && mv dist/odata-v4-ng-app docs",
    "versioningApp": "git add -A && git commit -m 'docs: update' && standard-version --release-as 1.0.15",
    "buildAll": "npm run versioningLib && npm run buildLib && npm run buildApp && npm run versioningApp",
    "publishApp": "git push --follow-tags origin master",
    "publishLib": "npm publish dist/odata-v4-ng",
    "publishAll": "npm run publishApp && npm run publishLib",
    "release": "cp README.md projects/odata-v4-ng/README.md && npm run buildAll && npm run publishAll"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.5.4",
    "primeicons": "^1.0.0-beta.10",
    "primeng": "^6.1.2",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.3.0",
    "rxjs-tslint": "^0.1.5",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
