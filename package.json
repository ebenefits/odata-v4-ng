{
  "name": "@ebenefitsbv/odata-v4-ng",
  "version": "20.3.0",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular, continuance on the version originally built by Riccardo Mariani",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebenefits/odata-v4-ng/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular"
  ],
  "author": "eBenefits B.V.",
  "bugs": {
    "url": "https://github.com/ebenefits/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/ebenefits/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tag-package-version": "npm version 20.3.0 --allow-same-version --git-tag-version && node scripts/align-package-version-number.js",
    "buildLib": "ncp README.md projects/odata-v4-ng/README.md && ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --configuration=production",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --configuration=production && rimraf docs && ncp dist/odata-v4-ng-app docs && rimraf dist/odata-v4-ng-app",
    "buildAll": "npm run buildLib && npm run buildApp",
    "publish": "npm run tag-package-version && npm run buildLib && cd dist/odata-v4-ng && npm publish --access public && cd ../../"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^20.3.9",
    "@angular/cdk": "^20.2.11",
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@fullcalendar/core": "^6.1.19",
    "@primeng/themes": "^20.3.0",
    "@primeuix/themes": "^1.2.5",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.7.0",
    "uuid": "^13.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.9",
    "@angular-eslint/builder": "^20.5.1",
    "@angular-eslint/eslint-plugin": "^20.5.1",
    "@angular-eslint/eslint-plugin-template": "^20.5.1",
    "@angular-eslint/schematics": "^20.5.1",
    "@angular-eslint/template-parser": "^20.5.1",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.9",
    "@angular/language-service": "^20.3.9",
    "@types/jasmine": "^5.1.12",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.19.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "jasmine-core": "^5.12.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "move-cli": "^2.0.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^20.3.0",
    "protractor": "^7.0.0",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
