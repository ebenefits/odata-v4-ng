{
  "name": "@ebenefitsbv/odata-v4-ng",
  "version": "19.0.3",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular, continuance on the version originally built by Riccardo Mariani",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebenefits/odata-v4-ng/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular"
  ],
  "author": "eBenefits B.V.",
  "bugs": {
    "url": "https://github.com/ebenefits/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/ebenefits/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng build odata-v4-ng && ng serve",
    "build": "npm run buildAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tag-package-version": "npm version 19.0.2 --allow-same-version --git-tag-version && node scripts/align-package-version-number.js",
    "buildLib": "ncp README.md projects/odata-v4-ng/README.md && ng lint odata-v4-ng && ng test odata-v4-ng && ng build odata-v4-ng --configuration=production",
    "buildApp": "ng lint odata-v4-ng-app && ng test odata-v4-ng-app && ng build odata-v4-ng-app --configuration=production && rimraf docs && move-cli dist/odata-v4-ng-app docs",
    "buildAll": "npm run buildLib && npm run buildApp",
    "publish": "npm run tag-package-version && npm run buildLib && cd dist/odata-v4-ng && npm publish --access public && cd ../../"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/router": "^19.0.1",
    "@fullcalendar/core": "^6.1.15",
    "@primeng/themes": "^19.0.1",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.1",
    "rxjs": "^7.5.4",
    "tslib": "^2.7.0",
    "uuid": "^11.0.3",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@angular/language-service": "^19.0.1",
    "@types/jasmine": "^5.1.5",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "jasmine-core": "^5.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "move-cli": "^2.0.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^19.0.1",
    "protractor": "^7.0.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
