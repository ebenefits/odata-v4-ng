{
  "name": "odata-v4-ng",
  "version": "1.0.3",
  "license": "MIT",
  "description": "Client side OData V4 typescript library for Angular2+",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riccardomariani/odata-v4-ng.git"
  },
  "keywords": [
    "client",
    "odata",
    "odata v4",
    "typescript",
    "angular",
    "angular2",
    "angular3",
    "angular4",
    "angular5"
  ],
  "author": "Riccardo Mariani",
  "bugs": {
    "url": "https://github.com/riccardomariani/odata-v4-ng/issues"
  },
  "homepage": "https://github.com/riccardomariani/odata-v4-ng#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "buildProject": "npm install && npm prune && npm list --depth=0 && ng lint && ng test --single-run --code-coverage && ng e2e && ng build --deploy-url=odata-v4-ng && rm -R -f docs && mv dist docs",
    "packageProject": "ng-packagr -p ng-package.json",
    "publishDist": "npm publish dist",
    "buildPackagePublish": "npm run buildProject && npm run packageProject && npm run publishDist"
  },
  "private": false,
  "peerDependencies": {
    "@angular/common": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "angular2-uuid": "^1.1.1"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "font-awesome": "^4.7.0",
    "primeng": "^5.0.0",
